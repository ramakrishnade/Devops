pipeline {
    agent any

    stages {
        stage('check out SCM') {
            steps {
                echo 'cloning code from github'
            }
        }
		steps('Build'){
			step{
				echo 'build application'
			}
			
		}
		stage('test'){
			steps{
				echo 'Test the Applucation'
			
			}
		}
    }
}
